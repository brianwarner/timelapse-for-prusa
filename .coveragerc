[run]
source = .
omit =
    */tests/*
    */venv/*
    */.venv/*
    */site-packages/*
    setup.py
    */__pycache__/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abstract

[html]
directory = htmlcov
